{
  "expo": {
    "name": "Ron Turgeman",
    "slug": "barber_app",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "barberapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.orelaharon.rontugemanbarber",
      "googleServicesFile": "./app/GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "האפליקציה זקוקה לגישה לתמונות כדי להעלות תמונות לגלריה",
        "NSCameraUsageDescription": "האפליקציה זקוקה לגישה למצלמה כדי לצלם תמונות"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.orelaharon.rontugemanbarber",
      "googleServicesFile": "./app/google-services.json",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ],
      "expo-notifications",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "האפליקציה זקוקה לגישה לתמונות כדי להעלות תמונות לגלריה",
          "cameraPermission": "האפליקציה זקוקה לגישה למצלמה כדי לצלם תמונות"
        }
      ]
    ],
    "runtimeVersion": "1.1.6",
    "updates": {
      "url": "https://u.expo.dev/229bec8c-f551-41b7-8e6b-e8e26fb31945"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "229bec8c-f551-41b7-8e6b-e8e26fb31945"
      }
    },
    "owner": "orel895"
  }
}
